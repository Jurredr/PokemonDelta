{
    "name": "pokemon_delta",
    "description": "",
    "version": "1.0.0",
    "repository": "https://github.com/Jurredr/PokemonDelta.git",
    "scripts": {
        "lint": "eslint --ignore-path .gitignore .",
        "lint-fix": "eslint --ignore-path .gitignore --ignore-path .eslintignore . --fix",
        "public:serve": "parcel public/index.html --out-dir dist/public",
        "public:watch": "parcel watch public/index.html --out-dir dist/public",
        "public:build": "parcel build public/index.html --out-dir dist/public",
        "server:start": "babel-node server/index.js",
        "server:watch": "nodemon --exec babel-node server/index.js",
        "watch": "concurrently \"yarn run public:watch\" \"yarn run server:watch\"",
        "start": "yarn run public:build && yarn run server:start"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/node": "^7.10.5",
        "@babel/preset-env": "^7.11.5",
        "babel-eslint": "^10.1.0",
        "concurrently": "^5.3.0",
        "eslint": "^7.10.0",
        "eslint-plugin-prettier": "^3.1.4",
        "nodemon": "^2.0.4",
        "parcel-bundler": "^1.12.4",
        "prettier": "^2.1.2"
    },
    "dependencies": {
        "express": "^4.17.1"
    }
}
